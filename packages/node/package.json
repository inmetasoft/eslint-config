{
  "name": "@inmeta/eslint-config-node",
  "main": "index.js",
  "version": "1.1.0",
  "description": "INMETA ESLint config for Node",
  "license": "MIT",
  "repository": "https://github.com/inmetasoft/eslint-config/tree/master/packages/node",
  "author": "Gabriel <gabriel-peruchi@hotmail.com> (https://github.com/gabriel-peruchi)",
  "homepage": "https://github.com/inmetasoft/eslint-config/tree/master/packages/node#README",
  "scripts": {
    "release": "release-it"
  },
  "dependencies": {
    "@inmeta/eslint-config-base": "1.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "release-it": "^16.0.0"
  },
  "peerDependencies": {
    "eslint": "^7.23.0 || ^8.0.0",
    "typescript": ">=3.3.1"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "@release-it/bumper": {
        "out": {
          "file": "package.json",
          "path": [
            "dependencies.@inmeta/eslint-config-base"
          ]
        }
      }
    }
  }
